{
  "version": 2,
  "builds": [
    { "src": "next.config.mjs", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE": "@next_public_api_base",
    "NEXT_PUBLIC_BASE_URL": "@next_public_base_url"
  }
}
